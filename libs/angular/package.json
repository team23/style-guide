{
    "name": "@team23/eslint-config-team23-angular",
    "version": "17.1.0-rc3",
    "description": "A set of eslint rules used by TEAM23 for standard angular projects",
    "keywords": [
        "eslint",
        "angular",
        "TEAM23"
    ],
    "author": "TEAM23 GmbH",
    "license": "MIT",
    "homepage": "https://github.com/team23/style-guide/libs/angular",
    "repository": {
        "type": "git",
        "url": "https://github.com/team23/style-guide.git",
        "directory": "libs/angular"
    },
    "scripts": {
        "build": "rm -rf ./dist/* && npx tsc-multi --force"
    },
    "files": [
        "./dist/**/*",
        "./*.md",
        "./package.json",
        "./LICENSE",
        "!./tsconfig*.json",
        "!./dist/**/*.tsbuildinfo"
    ],
    "type": "module",
    "exports": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/mjs/index.mjs",
        "require": "./dist/cjs/index.cjs",
        "default": "./dist/mjs/index.cjs"
    },
    "dependencies": {
        "angular-eslint": "^18.1.0",
        "typescript-eslint": "^8.0.0-alpha.20"
    },
    "devDependencies": {
        "@angular/core": "^17.2.1",
        "@types/eslint": "^8.56.10",
        "@types/eslint__eslintrc": "^2.1.1",
        "@types/eslint__js": "^8.42.3",
        "tsc-multi": "^1.1.0"
    },
    "peerDependencies": {
        "eslint": "^8.56.0"
    }
}
