{
    "name": "@team23/eslint-config-team23-angular",
    "version": "20.1.3",
    "description": "A set of eslint rules used by TEAM23 for angular projects",
    "keywords": [
        "eslint",
        "eslintconfig",
        "TEAM23",
        "angular"
    ],
    "author": "TEAM23 GmbH",
    "license": "MIT",
    "homepage": "https://github.com/team23/style-guide/tree/main/libs/angular",
    "repository": {
        "type": "git",
        "url": "https://github.com/team23/style-guide.git",
        "directory": "libs/angular"
    },
    "scripts": {
        "build": "rm -rf ./dist/* && npx tsc-multi --force"
    },
    "files": [
        "./dist/**/*",
        "./*.md",
        "./package.json",
        "./LICENSE"
    ],
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
    },
    "dependencies": {
        "angular-eslint": "^20.1.1",
        "typescript-eslint": "^8.39.0"
    },
    "devDependencies": {
        "@angular/core": "^21.0.1",
        "@types/eslint": "^9.6.1",
        "jiti": "^2.5.1",
        "tsc-multi": "^1.1.0"
    },
    "peerDependencies": {
        "eslint": "^9.21.0"
    }
}
