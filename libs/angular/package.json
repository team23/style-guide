{
    "name": "@team23/eslint-config-team23-angular",
    "version": "19.0.2",
    "description": "A set of eslint rules used by TEAM23 for angular projects",
    "keywords": [
        "eslint",
        "eslintconfig",
        "TEAM23",
        "angular"
    ],
    "author": "TEAM23 GmbH",
    "license": "MIT",
    "homepage": "https://github.com/team23/style-guide/tree/main/libs/angular",
    "repository": {
        "type": "git",
        "url": "https://github.com/team23/style-guide.git",
        "directory": "libs/angular"
    },
    "scripts": {
        "build": "rm -rf ./dist/* && npx tsc-multi --force"
    },
    "files": [
        "./dist/**/*",
        "./*.md",
        "./package.json",
        "./LICENSE",
        "!./tsconfig*.json",
        "!./dist/**/*.tsbuildinfo"
    ],
    "type": "module",
    "exports": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/mjs/index.mjs",
        "require": "./dist/cjs/index.cjs",
        "default": "./dist/mjs/index.mjs"
    },
    "dependencies": {
        "angular-eslint": "^19.1.0",
        "typescript-eslint": "^8.25.0"
    },
    "devDependencies": {
        "@angular/core": "^19.2.0",
        "@types/eslint": "^9.6.1",
        "@types/eslint__eslintrc": "^2.1.2",
        "jiti": "^2.4.2",
        "tsc-multi": "^1.1.0"
    },
    "peerDependencies": {
        "eslint": "^9.21.0"
    }
}
