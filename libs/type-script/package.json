{
    "name": "@team23/eslint-config-team23-ts",
    "version": "9.0.0-rc7",
    "description": "A set of eslint rules used by TEAM23 for standard ts projects",
    "keywords": [
        "eslint",
        "eslintconfig",
        "TEAM23",
        "typescript",
        "ts"
    ],
    "author": "TEAM23 GmbH",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/team23/style-guide.git",
        "directory": "libs/type-script"
    },
    "scripts": {
        "build": "rm -rf ./dist/* && npx tsc-multi --force"
    },
    "files": [
        "./dist/**/*",
        "./*.md",
        "./package.json",
        "./LICENSE",
        "!./tsconfig*.json",
        "!./dist/**/*.tsbuildinfo"
    ],
    "type": "module",
    "exports": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/mjs/index.mjs",
        "require": "./dist/cjs/index.cjs",
        "default": "./dist/mjs/index.cjs"
    },
    "dependencies": {
        "@stylistic/eslint-plugin": "^2.1.0",
        "tsc-multi": "^1.1.0",
        "typescript-eslint": "^7.10.0"
    },
    "devDependencies": {
        "@types/eslint": "^8.56.10",
        "@types/eslint__js": "^8.42.3"
    },
    "peerDependencies": {
        "@stylistic/eslint-plugin": "^2.1.0",
        "eslint": "^8.56.0",
        "typescript-eslint": "^7.10.0"
    }
}
