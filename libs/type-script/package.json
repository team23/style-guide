{
    "name": "@team23/eslint-config-team23-ts",
    "version": "10.2.2",
    "description": "A set of eslint rules used by TEAM23 for standard ts projects",
    "keywords": [
        "eslint",
        "eslintconfig",
        "TEAM23",
        "typescript",
        "ts"
    ],
    "author": "TEAM23 GmbH",
    "license": "MIT",
    "homepage": "https://github.com/team23/style-guide/tree/main/libs/type-script",
