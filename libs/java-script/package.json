{
    "name": "@team23/eslint-config-team23-standard",
    "version": "9.0.0-rc4",
    "description": "A set of eslint rules used by TEAM23 for standard js projects",
    "keywords": [
        "eslint",
        "eslint-config",
        "TEAM23",
        "js"
    ],
    "author": "TEAM23 GmbH",
    "license": "MIT",
    "homepage": "https://github.com/team23/style-guide/libs/java-script",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/team23/style-guide.git",
        "directory": "libs/java-script"
    },
    "scripts": {
        "build": "rm -rf ./dist/* && npx tsc-multi --force"
    },
    "files": [
        "./dist/**/*",
        "./*.md",
        "./package.json",
        "./LICENSE",
        "!./tsconfig*.json",
        "!./dist/**/*.tsbuildinfo"
    ],
    "type": "module",
    "exports": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/mjs/index.mjs",
        "require": "./dist/cjs/index.cjs",
        "default": "./dist/mjs/index.cjs"
    },
    "dependencies": {
        "@eslint/js": "^8.57.0",
        "@stylistic/eslint-plugin-js": "^2.1.0",
        "eslint-plugin-import-x": "^0.5.1",
        "eslint-plugin-jsdoc": "^48.2.9",
        "globals": "^15.4.0",
        "typescript-eslint": "^7.10.0"
    },
    "devDependencies": {
        "@types/eslint": "^8.56.10",
        "@types/eslint__js": "^8.42.3",
        "eslint": "^8.56.0",
        "tsc-multi": "^1.1.0"
    },
    "peerDependencies": {
        "eslint": "^8.56.0"
    }
}
